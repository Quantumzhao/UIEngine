<Window x:Class="FrontEnd.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FrontEnd"
        mc:Ignorable="d"
		Loaded="Window_Loaded"
        Title="MainWindow" Height="450" Width="800">
	<Grid>
		<StackPanel x:Name="MainPanel" Orientation="Vertical" Height="Auto" VerticalAlignment="Top">
		</StackPanel>
		<Button x:Name="Add" Content="Add" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,10,10.5" Click="Add_Click"></Button>
	</Grid>
</Window>
